    "def evens_and_odds(n: int) -> dict:\n",
    "    evens_sum = 0\n",
    "    odds_sum = 0\n",
    "\n",
    "    for i in range(n):\n",
    "        if i % 2 == 0:\n",
    "            evens_sum += i\n",
    "        else:\n",
    "            odds_sum += i\n",
    "            \n",
    "    return {'evens': evens_sum, 'odds': odds_sum}\n"
   ]
